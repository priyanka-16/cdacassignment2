#include<iostream>
using namespace std;
int main()
{
	int a[10];
	int n;
	cin>>n;
	for(int i=0;i<n;i++)
	{

		cin>>a[i];
	}
	int temp;
	for(int i=0;i<n;i++)
	{
		for(int j=n;j<0;j--)
		{
			temp=a[j];
			a[j]=a[i];
			a[i]=temp;
			cout<<"jj";
			if(i==j)
				break;
		}
	}
	for(int i=0;i<n;i++)
	{
	cout<<a[i];
	}
return 0;
}
